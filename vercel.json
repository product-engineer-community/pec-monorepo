{
  "redirects": [
    {
      "source": "/",
      "destination": "https://productengineer.info/community"
    }
  ],
  "rewrites": [
    {
      "source": "/auth/:path*",
      "destination": "https://auth.productengineer.info/auth/:path*"
    },
    {
      "source": "/camp/:path*",
      "destination": "https://camp.productengineer.info/camp/:path*"
    },
    {
      "source": "/community/:path*",
      "destination": "https://community.productengineer.info/:path*"
    }
  ]
}
